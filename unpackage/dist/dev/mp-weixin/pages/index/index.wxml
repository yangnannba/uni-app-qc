<view class="content"><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{slides}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><swiper-item><image class="item-img" src="{{item.image}}"></image><view class="content-wrap"><view class="sub_header">{{item.sub_header}}</view><view class="header">{{item.header}}</view><view class="description">{{item.description}}</view></view><view class="mores"><text data-event-opts="{{[['tap',[['yuyue']]]]}}" bindtap="__e">预约驾驶</text><text data-event-opts="{{[['tap',[['todetails',['$0'],[[['slides','id',item.id,'id']]]]]]]}}" bindtap="__e">查看详情</text></view></swiper-item></block></swiper><view class="vehicles"><block wx:for="{{vehicles}}" wx:for-item="item" wx:for-index="__i1__" wx:key="id"><view data-event-opts="{{[['tap',[['todetails',['$0'],[[['vehicles','id',item.id,'id']]]]]]]}}" class="vehicles-item" bindtap="__e"><image src="{{item.image}}"></image><view class="content-wrap"><view class="header">{{item.header}}</view><view class="sub_header">{{item.sub_header}}</view><view class="description">{{item.description}}</view></view></view></block></view></view>