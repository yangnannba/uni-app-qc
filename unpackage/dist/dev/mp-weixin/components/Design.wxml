<view class="design" style="{{'background:'+(background)+';'}}"><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" duration="{{duration}}"><block wx:for="{{design}}" wx:for-item="item" wx:for-index="__i0__"><swiper-item class="swiper-item"><view class="title">{{title}}</view><view class="header">{{item.header}}</view><view class="description">{{item.description}}</view><image class="image" src="{{item.image}}" data-event-opts="{{[['tap',[['showimg',['$0'],[[['design','',__i0__,'image']]]]]]]}}" bindtap="__e"></image></swiper-item></block></swiper></view>